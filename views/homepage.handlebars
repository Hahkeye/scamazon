
<div id="slider" class="container" >
  {{#each product2}}
  <div class="m-2">
    <img src="assets/images/{{img_name}}"  alt="..." width="250px" height="250px">
    <h5 class="card-title">{{name}}</h5>
  </div>
  {{/each}}
</div>

<p class="top">Top Sellers</p>

<div id="cardContainer" class="container d-flex justify-content-center">
  {{#each product}}
    <div class="card" >
        <img src="assets/images/{{img_name}}" class="card-img-top2" alt="..." width="250px" height="250px">
          <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <div class="d-flex row position-relative p-3">
              <img src="/assets/images/download.jpg" width="30" height="30">
              <p class="card-text">{{price}}</p>
            </div>
            <a href="/product/get/{{id}}" class="btn btn-primary">Go somewhere</a>
          </div>
    </div>
  {{/each}}
</div>
 
<script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
				
<script "text/javascript" src="assets/js/homePage.js"></script>
